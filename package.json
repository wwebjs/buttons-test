{
  "name": "buttons-test",
  "version": "1.1.0",
  "description": "A utility to diagnose and study how buttons behave.",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "buttons-test": "npm run build && node --inspect ./dist/index",
    "dev": "nodemon --watch './**/*.ts' --ext ts --exec \"node --inspect -r ts-node/register src/index.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wwebjs/buttons-test.git"
  },
  "keywords": [
    "whatsapp-web.js",
    "buttons",
    "diagnosis"
  ],
  "author": "PurpShell (under the WWebJS github organization)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wwebjs/buttons-test/issues"
  },
  "homepage": "https://github.com/wwebjs/buttons-test#readme",
  "dependencies": {
    "qrcode-terminal": "^0.12.0",
    "whatsapp-web.js": "github:pedroslopez/whatsapp-web.js#fix-buttons-list"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
